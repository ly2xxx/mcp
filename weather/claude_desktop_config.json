            "env": {
				"EXCEL_FILES_PATH": "C:\\Users\\ly2xx\\Downloads"

			}
        },
        "weather": {
            "command": "uv",
            "args": [
                "--directory",
                "E:\\code2\\mcp2\\weather\\",
